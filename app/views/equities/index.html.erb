<main>
  <div class="panel" style="margin: 10px; padding: 20px;">
  
<p id="notice"><%= notice %></p>

<h1>Equities</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>Risk</th>
      <th>Certificat number</th>
      <th>Issue date</th>
      <th>Equity type</th>
      <th>Equity period</th>
      <th>Bill currency</th>
      <th>Agency code</th>
      <th>Passport number</th>
      <th>Profession</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @equities.each do |equity| %>
      <tr>
        <td><%= equity.customer_id %></td>
        <td><%= equity.risk_id %></td>
        <td><%= equity.certificat_number %></td>
        <td><%= equity.issue_date %></td>
        <td><%= equity.equity_type_id %></td>
        <td><%= equity.equity_period %></td>
        <td><%= equity.bill_currency %></td>
        <td><%= equity.agency_code %></td>
        <td><%= equity.passport_number %></td>
        <td><%= equity.profession %></td>
        <td><%= link_to 'Show', equity %></td>
        <td><%= link_to 'Edit', edit_equity_path(equity) %></td>
        <td><%= link_to 'Destroy', equity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Equity', new_equity_path %>


  </div>
</main>